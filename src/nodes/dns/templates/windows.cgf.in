#
#	startup configuration for @PROJECT_NAME@ dns node
#	created : ${_NOW_UTC}
#	platform: @CMAKE_SYSTEM@
#
[service]
enabled = false
name = ${lowercase_project_name}_${PROJECT_NAME}_v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}

[setup]
json = $ENV{ProgramData}\solosTec\SMF\dns_v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.json
pool-size = ${${PROJECT_NAME}_POOL_SIZE}
