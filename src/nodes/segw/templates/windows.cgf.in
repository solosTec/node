#
#	startup configuration for @PROJECT_NAME@ smart energy gateway
#	created : ${_NOW_UTC}
#	platform: @CMAKE_SYSTEM@
#
[service]
enabled = false
name = ${lowercase_project_name}_${PROJECT_NAME}_v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}

[setup]
json = $ENV{ProgramData}\solosTec\SMF\segw_v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.json
pool-size = ${${CMAKE_PROJECT_NAME}_POOL_SIZE}
config-index = 0
