#include "${CMAKE_BINARY_DIR}/include/smf.h"
#define APSTUDIO_READONLY_SYMBOLS
#include "winres.h"
#undef APSTUDIO_READONLY_SYMBOLS

LANGUAGE LANG_GERMAN, SUBLANG_GERMAN


VS_VERSION_INFO VERSIONINFO
 FILEVERSION ${${CMAKE_PROJECT_NAME}_VERSION_MAJOR},${${CMAKE_PROJECT_NAME}_VERSION_MINOR},0,${${CMAKE_PROJECT_NAME}_VERSION_PATCH}
 PRODUCTVERSION ${${CMAKE_PROJECT_NAME}_VERSION_MAJOR},${${CMAKE_PROJECT_NAME}_VERSION_MINOR},0,${${CMAKE_PROJECT_NAME}_VERSION_PATCH}
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x40004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040004b0"
        BEGIN
            VALUE "CompanyName", "solosTec"
            VALUE "FileDescription", "${PROJECT_NAME}/${CMAKE_PROJECT_NAME} node"
            VALUE "FileVersion", "${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.${${CMAKE_PROJECT_NAME}_VERSION_PATCH}.${${CMAKE_PROJECT_NAME}_VERSION_TWEAK}"
            VALUE "InternalName", "${CMAKE_PROJECT_NAME}.${PROJECT_NAME}"
            VALUE "LegalCopyright", "Copyright (C) ${_THIS_YEAR} solosTec"
            VALUE "OriginalFilename", "${PROJECT_NAME}.exe"
            VALUE "ProductName", "${PROJECT_NAME}"
            VALUE "ProductVersion", "${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.${${CMAKE_PROJECT_NAME}_VERSION_PATCH}.${${CMAKE_PROJECT_NAME}_VERSION_TWEAK}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x400, 1200
    END
END

IDI_ICON1               ICON                    "${ASSET_LOGO_ICO_PATH}"