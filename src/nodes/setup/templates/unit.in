[Unit]
Description=${PROJECT_NAME} server v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.${${PROJECT_NAME}_VERSION_PATCH} (project ${PROJECT_NAME})
Wants=node-master.service
After=network.target smf-master.service

[Service]
Type=simple
ExecStart=/opt/smf/${PROJECT_NAME} -C/opt/smf/${PROJECT_NAME}_v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.cfg
Restart=on-failure
RestartSec=12

[Install]
WantedBy=multi-user.target
