#
#	startup configuration for @PROJECT_NAME@ lora node
#	created : ${_NOW_UTC}
#	platform: @CMAKE_SYSTEM@
#
[service]
enabled = false
name = smf_lora_v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}

[setup]
json = $ENV{ProgramData}\solosTec\SMF\lora_v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.json
pool-size = ${${CMAKE_PROJECT_NAME}_POOL_SIZE}

[x509]
C = CH
L = Lucerne
O = solosTec
CN = solostec.net
