[Unit]
Description=dash server v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.${${PROJECT_NAME}_VERSION_PATCH} (project ${PROJECT_NAME})
After=network.target

[Service]
Type=simple
ExecStart=/opt/smf/dash -C/opt/smf/dash_v${${CMAKE_PROJECT_NAME}_VERSION_MAJOR}.${${CMAKE_PROJECT_NAME}_VERSION_MINOR}.cfg
Restart=on-failure
RestartSec=12

[Install]
WantedBy=multi-user.target
